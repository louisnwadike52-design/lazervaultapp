{
    "name": "lazervault",
    "version": "1.0.0",
    "description": "LazerVault Frontend",
    "main": "lib/main.dart",
    "scripts": {
        "build": "flutter build",
        "start": "flutter run",
        "dev": "flutter run -d emulator-5554",
        "test": "echo \"Error: no test specified\" && exit 1",
        "analyze": "flutter analyze",
        "generate:proto": "export PATH=\"$PATH\":\"$HOME/.pub-cache/bin\" && ./generate_proto.sh",
