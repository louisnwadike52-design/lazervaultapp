        "deep:clean": "flutter clean && rm -rf .dart_tool/ build/ && rm -rf ios/Pods ios/Podfile.lock && flutter pub get && flutter clean && flutter pub get && flutter pub run build_runner build",
        "splash:generate": "flutter pub run flutter_native_splash:create"
    },
