    "dependencies": {
        "flutter": "^3.10.0",
        "grpc": "^1.56.0",
        "protobuf": "^3.20.3",
        "protoc-gen-dart": "^2.15.0",
        "grpc-web": "^2.1.0",
        "grpc-web-generator": "^0.2.0",
        "googleapis": "^11.0.0"
    },
    "devDependencies": {
        "protoc-gen-dart": "^2.15.0"
    }
