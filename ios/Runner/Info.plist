<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>Lazervaultapp</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>lazervaultapp</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<key>LSRequiresIPhoneOS</key>
		<true/>
		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>
		<key>UIMainStoryboardFile</key>
		<string>Main</string>
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>UISupportedInterfaceOrientations~ipad</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationPortraitUpsideDown</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true/>
		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true/>
		<key>UIStatusBarHidden</key>
		<false/>
		<key>NSCameraUsageDescription</key>
		<string>$(PRODUCT_NAME) uses your camera</string>
		<key>NSMicrophoneUsageDescription</key>
		<string>We need access to your microphone for voice enrollment and secure voice biometric authentication.</string>
		<key>NSContactsUsageDescription</key>
		<string>LazerVault needs access to your contacts to help you quickly send money to friends and family.</string>
		<key>UIBackgroundModes</key>
		<array>
			<string>audio</string>
		</array>
		<!-- Deep link URL scheme for lazervault:// -->
		<!-- Used for DirectPay authorization callbacks and other in-app redirects -->
		<key>CFBundleURLTypes</key>
		<array>
			<dict>
				<key>CFBundleTypeRole</key>
				<string>Editor</string>
				<key>CFBundleURLName</key>
				<string>app.lazervault</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<string>lazervault</string>
				</array>
			</dict>
		</array>
		<!-- Universal Links - Associated Domains configured in Xcode -->
		<key>FlutterDeepLinkingEnabled</key>
		<true/>
		<!-- Siri integration for voice-activated payments -->
		<key>NSSiriUsageDescription</key>
		<string>LazerVault uses Siri to let you quickly send money, check your balance, and pay bills with your voice.</string>
		<!-- App Icon Quick Actions (long-press shortcuts) -->
		<key>UIApplicationShortcutItems</key>
		<array>
			<dict>
				<key>UIApplicationShortcutItemType</key>
				<string>send_money</string>
				<key>UIApplicationShortcutItemTitle</key>
				<string>Send Money</string>
				<key>UIApplicationShortcutItemSubtitle</key>
				<string>Transfer funds instantly</string>
				<key>UIApplicationShortcutItemIconType</key>
				<string>UIApplicationShortcutIconTypeShuffle</string>
			</dict>
			<dict>
				<key>UIApplicationShortcutItemType</key>
				<string>scan_to_pay</string>
				<key>UIApplicationShortcutItemTitle</key>
				<string>Scan to Pay</string>
				<key>UIApplicationShortcutItemSubtitle</key>
				<string>QR code payment</string>
				<key>UIApplicationShortcutItemIconType</key>
				<string>UIApplicationShortcutIconTypeCapturePhoto</string>
			</dict>
			<dict>
				<key>UIApplicationShortcutItemType</key>
				<string>check_balance</string>
				<key>UIApplicationShortcutItemTitle</key>
				<string>Check Balance</string>
				<key>UIApplicationShortcutItemSubtitle</key>
				<string>View account balance</string>
				<key>UIApplicationShortcutItemIconType</key>
				<string>UIApplicationShortcutIconTypeSearch</string>
			</dict>
			<dict>
				<key>UIApplicationShortcutItemType</key>
				<string>pay_bills</string>
				<key>UIApplicationShortcutItemTitle</key>
				<string>Pay Bills</string>
				<key>UIApplicationShortcutItemSubtitle</key>
				<string>Electricity, airtime &amp; more</string>
				<key>UIApplicationShortcutItemIconType</key>
				<string>UIApplicationShortcutIconTypeTask</string>
			</dict>
		</array>
	</dict>
</plist>
